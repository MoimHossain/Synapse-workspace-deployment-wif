    "azure-pipelines-tasks-azure-arm-rest": "3.263.1",
    "azure-pipelines-tasks-azure-arm-rest-v2": "2.0.4",
    "azure-pipelines-tasks-artifacts-common": "^2.262.0",
    "azure-devops-node-api": "^14.0.2",
    "typed-rest-client": "^1.8.4",
    "uuid": "8.3.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/mockery": "^1.4.29",
    "@types/uuid": "^8.3.0",
    "chai": "^4.2.0",
    "mockery": "^2.1.0",
  "moment": "^2.29.4",
