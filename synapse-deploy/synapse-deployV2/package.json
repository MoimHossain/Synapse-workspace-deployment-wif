{
  "name": "buildanddeploytask",
  "version": "1.0.0",
  "description": "Deploy plugin for synapse workspace",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "ci": "npx --yes rimraf node_modules && npm install --production",
    "cb": "npx --yes rimraf dist && tsc",
    "clean-js": "pwsh -NoProfile -ExecutionPolicy Bypass -Command \"Get-ChildItem -Path . -Include *.js, *.js.map -Recurse -File | Where-Object { $_.FullName -notlike '*node_modules*' } | Remove-Item -Force\"",
    "pubprep": "npm run ci && npm run cb"
  },
  "author": "MoimHossain",
  "license": "ISC",
